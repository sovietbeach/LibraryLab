class Book(object):
    def __init__(self,
                 title,
                 author,
                 year,
                 publisher,
                 location,
                 isbn,
                 num_copies):
        self.title = title
        self.author = author
        self.year = year
        self.publisher = publisher
        self.location = location
        self.isbn = isbn
        self.num_copies = num_copies
        
    def __str__(self):
        self.title + " " + self.author + " " + self.year + " " + self.publisher + " " + self.location + " " + self.isbn + " " + self.copies
